{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": ["standard", "plugin:@typescript-eslint/recommended","next/core-web-vitals", "eslint:recommended", "plugin:@typescript-eslint/recommended"],
  "env": {
    "browser": true,
    "node": true
  },
  "ignorePatterns": ["node_modules/", "dist/", "build/"],
  "globals": {
    "React": "writable"
  },
  "rules": {
    "@typescript-eslint/no-explicit-any": "off"
  }
}
