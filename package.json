{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "standard",
    "prettier": "prettier --check \"src/**/*.{js,jsx,json,md}\""
  },
  "dependencies": {
    "@types/react": "^18.0.28",
    "bootstrap": "^5.2.0",
    "bootstrap-icons": "^1.9.1",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "next": "^14.2.13",
    "react": "18.2.0",
    "react-bootstrap": "^2.5.0",
    "react-dom": "18.2.0",
    "sass": "^1.54.9",
    "swr": "1.1.2"
  },
  "devDependencies": {
    "@types/node": "^18.14.6",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.11.1",
    "eslint-config-next": "12.2.5",
    "husky": "^8.0.3",
    "prettier": "^2.8.4",
    "standard": "^17.1.2",
    "typescript": "^4.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier && lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "standard": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "env": {
      "browser": true,
      "node": true
    },
    "ignore": [
      "node_modules",
      "dist",
      "build"
    ],
    "globals": {
      "React": true
    }
  },
  "globals": {
    "React": "writable"
  }
}
